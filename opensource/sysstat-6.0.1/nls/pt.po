#: sadc.c:614
msgid "Cannot append data to that file\n"
msgstr "Não posso adicionar dados neste arquivo\n"

#: common.c:91
msgid "Cannot handle so many processors!\n"
msgstr "Não posso usar tantos processadores!\n"

